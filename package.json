    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-markdown": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@types/react": "^18.2.14",
    "react": "^18.2.0",
    "state-guard": "^4.0.0",
